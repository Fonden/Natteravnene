@model List<Event>

<div class="page-header position-relative">
    @if (NR.Infrastructure.CurrentProfile.isEditor | NR.Infrastructure.CurrentProfile.isSecretary)
    {
        @Html.ActionLink(General.BtnNewEvent, "Edit", null, null, new { @class = "btn btn-small btn-primary pull-right", data_rel="tooltip", data_placement = "left", title = General.BtnNewEventTooltip })
    }
    <h1>@General.Events</h1>
</div>

@{Html.RenderPartial("_EventListing", Model);}