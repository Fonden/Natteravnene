
<div class="page-header position-relative">

    
    <h1>Kommunikation</h1>

</div>


<div class="row">
    <div class="col-lg-6">
        <div class="widget-box">
            <div class="widget-header">
                <h4 class="widget-title">Version notification</h4>
            </div>

            <div class="widget-body">
                <div class="widget-main">
                   @using (Ajax.BeginForm("VersionNotification",  new AjaxOptions { UpdateTargetId = "result" }))
                        {
                        <input type="text" name="text" class="input-xxlarge" placeholder="Version" autocomplete="off">
                        <button type="submit" class="btn btn-info btn-sm">
                            <i class="ace-icon fa fa-key bigger-110"></i>Send
                        </button>
                        }
                    <div id="result"></div>
                </div>
            </div>
        </div>

    </div>
</div>